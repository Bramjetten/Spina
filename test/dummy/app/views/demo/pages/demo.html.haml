%h1= current_page.title

%h2= content :headline
%p!= content :body

%h5 Repeater
- repeater :repeater do |project|
  %h1= project.content :line
  %p= project.content :headline
  - project.content(:image_collection)&.each do |image|
    = spina_image_tag image, resize: '100x100'

%h4 Image
= spina_image_tag :image, resize: '100x100'

%h4 Image collection
- images :image_collection do |image|
  = spina_image_tag image, resize: '100x100'
